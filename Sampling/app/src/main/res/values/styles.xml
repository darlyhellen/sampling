<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="AppBaseTheme" parent="android:Theme.Holo.Light.DarkActionBar" />

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="notitle">
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="dialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:backgroundDimAmount">0.6</item>
    </style>

    <style name="xiangxunTheme" parent="@android:style/Theme.Black">
        <item name="android:windowBackground">@color/holo_gray_bright</item>
        <item name="android:colorBackground">@color/holo_gray_bright</item>
    </style>

    <style name="Linearlayout_Style">
        <item name="android:orientation">vertical</item>
        <item name="android:gravity">center</item>
        <item name="android:clickable">true</item>
        <item name="android:onClick">LayoutOnclick</item>
    </style>

    <style name="Theme.Light.FullScreenDialogAct" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ff555555</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="full_line_seperator">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1px</item>
        <item name="android:background">@color/full_line</item>
    </style>

    <style name="full_ver_line_seperator">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:background">@color/full_line</item>
    </style>

    <style name="custome_background" parent="@android:style/Theme.Light.NoTitleBar">
        <item name="android:windowBackground">@color/white</item>
    </style>

    <style name="detail_price_text" parent="android:Widget.TextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#e90a0b</item>
        <item name="android:textSize">@dimen/textSize15</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="PublishDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:paddingTop">40dp</item>
        <item name="android:paddingBottom">40dp</item>
    </style>

    <style name="full_ver_line_seperatorbyfublish">
        <item name="android:layout_width">1px</item>
        <item name="android:layout_height">20dp</item>
        <item name="android:background">#dcdcdc</item>
        <item name="android:layout_marginTop">16dp</item>
        <item name="android:layout_marginBottom">16dp</item>
        <item name="android:layout_gravity">center_vertical</item>
    </style>

    <style name="AnimationFade">
        <item name="android:windowEnterAnimation">@anim/popup_window_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_window_exit</item>
    </style>

    <style name="Add_Picture_AnimationFade">
        <item name="android:windowEnterAnimation">@anim/publish_push_bottom_in</item>
        <item name="android:windowExitAnimation">@anim/publish_push_bottom_out</item>
    </style>

    <!-- msgdialog -->
    <style name="msg_dalog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFrame">@null</item>
    </style>

    <style name="CustomCallDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:paddingLeft">15dp</item>
        <item name="android:paddingRight">15dp</item>
    </style>

    <style name="more_relativelayout">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">49dp</item>
        <item name="android:background">@drawable/more_item_selector</item>
        <item name="android:paddingLeft">15dp</item>
    </style>

    <style name="more_arrows" parent="android:Widget.TextView">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_width">16dp</item>
        <item name="android:layout_height">16dp</item>
        <item name="android:src">@mipmap/arrows</item>
    </style>

    <style name="public_write_text" parent="android:Widget.TextView">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/color333333</item>
        <item name="android:textSize">18sp</item>
        <item name="android:layout_marginLeft">4dp</item>
    </style>

    <style name="more_text2" parent="@style/more_text">
        <item name="android:textColor">#999999</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_marginRight">5dp</item>
    </style>

    <style name="more_text" parent="android:Widget.TextView">
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">16sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_marginLeft">20dp</item>
        <item name="android:layout_marginRight">20dp</item>
    </style>

    <style name="Default" parent="android:Theme" />

    <style name="NoTitle" parent="android:Theme.NoTitleBar">
        <item name="android:windowBackground">@color/color999999</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="FullScreen" parent="android:Theme.NoTitleBar.Fullscreen" />

    <style name="updateDialog" parent="@android:Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="preference_bg">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:background">@color/black</item>
        <item name="android:focusable">false</item>
        <item name="android:textColor">@color/color999999</item>
        <item name="android:cacheColorHint">@color/white</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="edittext">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">@dimen/margin_padding5</item>
        <item name="android:background">@drawable/login_edittext_normal</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:padding">@dimen/margin_padding5</item>
        <item name="android:cursorVisible">true</item>
    </style>

    <style name="TableLabel">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginTop">@dimen/margin_padding5</item>
        <item name="android:layout_marginBottom">@dimen/margin_padding3</item>
        <item name="android:background">@color/font_color</item>
        <item name="android:gravity">center_horizontal</item>
        <item name="android:padding">@dimen/margin_padding5</item>
        <item name="android:textSize">@dimen/font_size_15</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="TableLine">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">@dimen/margin_padding1</item>
        <item name="android:layout_marginLeft">@dimen/margin_padding10</item>
        <item name="android:layout_marginRight">@dimen/margin_padding10</item>
        <item name="android:background">@color/color_d1d1d1</item>
    </style>


    <style name="My.TextAppearance" parent="@style/TextAppearance.AppCompat.Widget.Switch">
        <item name="android:textSize">16sp</item>
        <item name="android:textColor">#ffa6b3bf</item>

    </style>


    <style name="time_dialog" parent="android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowBackground">@color/white</item>
    </style>
</resources>